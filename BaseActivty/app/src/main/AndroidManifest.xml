<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.baseactivty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".Base.BaseApplication"
        android:theme="@style/AppTheme">
        <!--开屏activity-->
        <activity android:name=".SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--主activity-->
        <activity android:name=".MainActivity">
        </activity>
        <!--轮播图activity-->
        <activity android:name=".Banner.BannerImageActivity"/>
        <!--recyclerview的下拉刷新上拉加载更多-->
        <activity android:name=".SWrecyclerview.SwRecyclerViewActivity"/>
        <!--底部标题栏的显示与隐藏-->
        <activity android:name=".Demo.bottom_tab_toggle.TabActivity"/>
        <!--广告倒计时-->
        <activity android:name=".Demo.CountDownViewActivity"/>
    </application>

</manifest>